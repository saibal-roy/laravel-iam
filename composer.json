{
    "name": "saibal-roy/laravel-iam",
    "description": "An elegant way to manage the identity access management for the Laravel framework",
    "keywords": [
        "Laravel",
        "Laravel rbac implementation with gui",
        "Identity access management",
        "Impersonate users login"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Saibal Roy",
            "email": "sunnyroy21@gmail.com",
            "homepage": "https://saibalroy.com"
        }
    ],
    "homepage": "https://github.com/saibal-roy/laravel-iam",
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "laravel/framework": "^5.6|^6.0|^7.0",
        "spatie/laravel-permission": "^2.37|^3.0",
        "laravelcollective/html": "~5.6.0|~5.7.0|~5.8.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelIam\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelIam\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelIam\\LaravelIamServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
